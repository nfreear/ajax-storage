{
  "name": "ajax-storage",
  "version": "0.1.0",
  "description": "Seamless HTML5 localStorage caching for jQuery Ajax requests.",
  "main": "ajax-storage.js",
  "dependencies": {
    "jquery": "*",
    "xmlhttprequest": "*"
  },
  "devDependencies": {
    "grunt": "~0.4.1",
    "grunt-contrib-jshint": "0.10.0",
    "grunt-contrib-uglify": "~0.2.2",
    "grunt-contrib-concat": "~0.3.0",
    "grunt-saucelabs": "*",
    "mocha-phantomjs": "*",
    "express": "^4.9.5",
    "minimist": "*",
    "request": "*",
    "mocha": "*",
    "chai": "*"
  },
  "scripts": {
    "start": "( node test/test-server.js --port=8010 ) &",
    "test": "mocha-phantomjs -R spec http://127.0.0.1:8010/test/test_runner.html",
    "ps": "ps aux | grep node # kill PID"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/nfreear/ajax-storage.git"
  },
  "keywords": [
    "jQuery",
    "ajax",
    "browser",
    "cache",
    "localStorage",
    "performance"
  ],
  "author": "Nick Freear <nfreear@yahoo.co.uk> (https://github.com/nfreear)",
  "license": "MIT",
  "readmeFilename": "README.md",
  "gitHead": "",
  "bugs": {
    "url": "https://github.com/nfreear/ajax-storage/issues"
  },
  "browser": {
    "xmlhttprequest": false
  },
  "testling": {
    "files": "test/*_test.js",
    "browsers": [
      "ie/8..latest",
      "chrome/36..latest",
      "firefox/31..latest",
      "opera/24..latest",
      "safari/latest",
      "firefox/nightly",
      "opera/next",
      "chrome/canary",
      "iphone/6",
      "ipad/6",
      "android-browser/latest"
    ],
    "harness": "mocha"
  }
}
